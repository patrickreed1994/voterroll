<%= flash[:notice] %>
<table>
  <thead>
    <tr>
      <th>PrecinctUID</th>
      <th>PrecinctName</th>
    </tr>
  </thead>
  <tbody>
    <% @precincts.each do |precinct| %>
      <tr>
        <td><%= precinct.PrecinctUID %></td>
        <td><%= precinct.PrecinctName %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div>
<h3>Import a Precinct CSV File</h3>
  <%= form_tag import_precincts_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>
</div>
<div>
  <br><%= link_to 'Add a Name CSV File', names_path %><br />
  <br><%= link_to 'Add a Segment CSV File', segments_path %><br />
  <br><%= link_to 'See Voter Roll', voters_path %><br />
</div>