<%= flash[:notice] %>
<table>
  <thead>
    <tr>
      <th>StSegUID</th>
      <th>StartHouseNumber</th>
      <th>EndHouseNumber</th>
      <th>OddEvenBoth</th>
      <th>StreetDirection</th>
      <th>StreetName</th>
      <th>StreetSuffix</th>
      <th>PrecinctUID</th>
    </tr>
  </thead>
  <tbody>
    <% @segments.each do |segment| %>
      <tr>
        <td><%= segment.StSegUID %></td>
        <td><%= segment.StartHouseNumber %></td>
        <td><%= segment.EndHouseNumber %></td>
        <td><%= segment.OddEvenBoth %></td>
        <td><%= segment.StreetDirection %></td>
        <td><%= segment.StreetName %></td>
        <td><%= segment.StreetSuffix %></td>
        <td><%= segment.PrecinctUID %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div>
<h3>Import a Segment CSV File</h3>
  <%= form_tag import_segments_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>
</div>
<div>
  <br><%= link_to 'Add a Name CSV File', names_path %><br />
  <br><%= link_to 'Add a Precinct CSV File', precincts_path %></br>
  <br><%= link_to 'See Voter Roll', voters_path %></br>
</div>